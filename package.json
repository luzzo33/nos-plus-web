{
  "name": "nos-plus-alpha",
  "version": "1.0.0-alpha",
  "private": true,
  "description": "NOS.plus Alpha continuous development build",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "next dev -p 3016",
    "build": "next build",
    "start": "next start -p 3016",
    "build:alpha": "DIST_DIR=.next-alpha next build",
    "start:alpha": "DIST_DIR=.next-alpha next start -p 3020 -H 0.0.0.0",
    "build:live": "NEXT_PUBLIC_ENV=live DIST_DIR=.next-live next build",
    "start:live": "DIST_DIR=.next-live next start -p 3040 -H 0.0.0.0",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "check:translations": "node scripts/check-translations.js",
    "translations:coverage": "node scripts/translation-coverage.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.25",
    "@radix-ui/react-dialog": "^1.1.14",
    "@tanstack/react-query": "^5.83.0",
    "@types/d3-sankey": "^0.12.4",
    "@vitalets/google-translate-api": "^9.2.1",
    "apexcharts": "^3.49.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.4",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-apexcharts": "1.4.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-window": "^1.8.11",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-window": "^1.8.8",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "glob": "^11.0.3",
    "patch-package": "^8.0.1",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "typescript": "^5"
  }
}
